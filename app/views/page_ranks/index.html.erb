<p id="notice"><%= notice %></p>

<h1>Page Ranks</h1>

<table>
  <thead>
    <tr>
      <th>Rank</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @page_ranks.each do |page_rank| %>
      <tr>
        <td><%= page_rank.rank %></td>
        <td><%= page_rank.link_id %></td>
        <td><%= link_to 'Show', page_rank %></td>
        <td><%= link_to 'Edit', edit_page_rank_path(page_rank) %></td>
        <td><%= link_to 'Destroy', page_rank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Page Rank', new_page_rank_path %>
